<div class="login-screen ">
	<div class="view">
		<div class="page">
			<div class="page-content login-screen-content">
				<div class="login-screen-title">Log In</div>
				<form id="login_form">
					<div class="list">
						<ul>
							<li class="item-content item-input">
								<div class="item-inner">
									<div class="item-title item-label">Username</div>
									<div class="item-input-wrap">
										<input type="text" name="username" placeholder="Your username">
									</div>
								</div>
							</li>
							<li class="item-content item-input">
								<div class="item-inner">
									<div class="item-title item-label">Password</div>
									<div class="item-input-wrap">
										<input type="password" name="password" placeholder="Your password">
									</div>
								</div>
							</li>
						</ul>
					</div>
					<p class="row"><span class="col-25"></span>
						<a id="sign_in_btn" onclick="app.navbar_view.login()" class="col-50 button button-raised">Sign In</a>
						<span class="col-25"></span></p>
					<p class="row"><span class="col-25"></span><a href="#" target="_blank" class="sign_up_anchor col-50 button button-raised link external">Create Account</a><span
								class="col-25"></span></p>
					<p class="text_center"><a href="#" class="login-screen-close">Close Login Screen</a></p>
				</form>
			</div>
		</div>
